/* variables colors for now implement later */
:root {
    --blue: #007bff;
    --indigo: #6610f2;
    --purple: #6f42c1;
    --pink: #e83e8c;
    --red: #ee5f5b;
    --orange: #fd7e14;
    --yellow: #f89406;
    --green: #008000;
    --teal: #20c997;
    --cyan: #5bc0de;
    --white: #fff;
    --black:#000;
    --gray: #7A8288;
    --gray-dark: #3A3F44;
    --primary: #3A3F44;
    --secondary: #7A8288;
    --success: #62c462;
    --info: #5bc0de;
    --warning: #f89406;
    --danger: #ee5f5b;
    --light: #e9ecef;
    --dark: #272B30;
    --goldSand:#e4b781;
/* alphas */
    --grayA:rgba(255, 255, 255, 0.349);
    --whiteA:rgba(255, 255, 255, 0.445);
    --whiteA1:rgba(255, 255, 255, 0.788);
/* brand */
    --brand:#0161b6;
    --brand-15:#005299;
    }
/* Printing in Canvas*/
@media print { a:link:after, a:visited:after { content: none; }}
/* Printing in Canvas*/

/*format background (image, color, etc.)*/
    body.ic-Login-Body{
    background-size: auto;
    background-attachment: fixed;
    background-color:var(--white);
    }

/*position the main login box area; This code sets it 75 pixels from the top*/
    .ic-Login__container{
        margin-top: 75px;
    }

    #forgot_passwors_form{
        display: visible !important;
    }

/*changed the size to 500x833 to accommodate the size of our background image*/
    .ic-Login__content{
        background: url(https://s3.amazonaws.com/scschoolfiles/1534/img_pd_131054_ufioxd.jpg);
        background-size:cover;
        background-position: center center;
        background-repeat: no-repeat;
        height: 500px; 
        width: 833px; 
        box-shadow: 0 4px 20px -2px var(--black);
        padding: 0px;
    }

    a.Button.Button--primary.ic-Login__sso-button--has-text.ic-Login__sso-button.ic-Login__sso-button--clever{
        background-color: var(--white);
    }
/* margin on forgot password */
    .ic-Form-control{
        margin-bottom: 5px !important;
    }
/* Hide Canvas header logo*/
    .ic-Login-header, .ic-Login-footer{
        display: visible !important;
    }
/* Login(parent) button right side and push footer down */
    /* .ic-Login__actions{
        display: flex;
        margin-bottom: 60px;
    } */
/* Login(parent) button left side remove padding*/
    .ic-Login__actions{
        display: block;
        margin-bottom: 0;
    }
/* Comment this section out if login button(parent) is moved to the right side*/
    .Button--login {
        margin-top: 10px;
    }
/* logout hide logo */
    .ic-Login-header__logo{
        display: none !important;
    }
/* pushed login down to keep in center recolored text for better visibility */
    .ic-Login__title{
        color: var(--black);
        margin-top: 200px;
    }
/* readibility "You are logged out of Canvas" */
    .ic-Login__text{
        color: var(--black);
    }

    .ic-Login-footer__logo-link{
        display: none !important;
    }
     
/*username login box*/
    #pseudonym_session_unique_id{
        height: 38px;
        width: 235px;
        background: var(--white);
    }
    
/*password login box*/
    #pseudonym_session_password{
        height: 38px;
        width: 235px;
        background: var(--white);
    }
    #forgot_password_instructions{
        background-color: var(--white)
    }
/*position entire login form (username, password, stay signed in, forgot password, log in button)*/
    #login_form{
        margin: 110px 0px 0px;
    }
/* login Button */
    .Button--login{
        background: var(--white) !important;
        margin-top: 5px;
    }
/*Forgot Password? link*/
    .ic-Login__link{
        color: var(--white) !important;
    }
    #forgot_password_form{
        margin-top: 140px;
    }
    .ic-Login__forgot-text {
        padding: 5px;
    }
/*Stay Signed In link*/
    label[for=pseudonym_session_remember_me]{
        color: var(--white) !important;
    }

/* Remove Email and password text */
    label[for="pseudonym_session_unique_id"]{
        display:visible;
        color: var(--black) !important;
    }
    label[for="pseudonym_session_password"]{
        display:visible;
        color: var(--black) !important;
    }
/* clever button */
    a.Button.Button--primary.ic-Login__sso-button--has-text.ic-Login__sso-button.ic-Login__sso-button--clever{
        will-change: auto;
        animation-name: noticeMeSenpai;
        animation-duration: 4s;
        animation-iteration-count: infinite;
        animation-direction: alternate;
    }
/* animation for button */
    @keyframes noticeMeSenpai{
        0% { box-shadow: 0 0 -10px var(--white)}
        40% { box-shadow: 0 0 20px var(--white)}
        60% { box-shadow: 0 0 20px var(--white)}
        100% { box-shadow: 0 0 -10px var(--white)}
    }

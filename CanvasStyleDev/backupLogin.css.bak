/* Mobile Application */
    .mobileLogin-Header{
        z-index: 1;
        background-image: none !important;
        background-position: center;
        background-repeat: no-repeat;
        background-size: cover;
    }

    .glow {
        will-change: auto;
        animation-name: noticeMeSenpai;
        animation-duration: 4s;
        animation-iteration-count: infinite;
        animation-direction: alternate;
    }

    .webkit.chrome.touch, .webkit.safari.touch{
        background-image: url(https://s3.amazonaws.com/scschoolfiles/1534/img_pd_131054_ufioxd.jpg) !important;
        background-position: center;
        background-repeat: no-repeat;
        background-size: cover;
    }
    .webkit.chrome.touch.with-right-side, .webkit.safari.touch.with-right-side{
        background-image: none !important;
    }
    .webkit.chrome.touch.primary-nav-expanded, .webkit.safari.touch.primary-nav-expanded{
        background-image: none !important;
    }
@supports (-webkit-overflow-scrolling: touch) {
    /* CSS specific to iOS devices */ 
    .touch.with-right-side{
        background-image: none !important;
    }
}
@supports not (-webkit-overflow-scrolling: touch) {
    /* CSS non iOS devices */
    .touch.with-right-side{
        background-image: none !important;
    }
}
